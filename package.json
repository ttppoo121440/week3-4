    "prettier-plugin-tailwindcss": "^0.6.5",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.2.2",
    "vite": "^5.3.1",
    "vite-plugin-ejs": "^1.7.0",
    "vite-tsconfig-paths": "^4.3.2"
